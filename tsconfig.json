{
  "compilerOptions": {
    "target": "ES6",
    "module": "esnext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      },
    ],
  },
  "include": ["src", ".astro/types.d.ts", "**/*"],
  "extends": "astro/tsconfigs/base",
  "exclude": ["dist"]
}


